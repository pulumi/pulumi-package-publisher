@keyframes notifications { 
  0%, 100% { transform: rotate(0deg); }
  20%, 60% { transform: rotate(7deg); }
  40%, 80% { transform: rotate(-7deg); }
}

@keyframes clockwise { 
  0% { transform: rotate(0deg); }
  100% { transform: rotate(1turn); }
}

@-webkit-keyframes clockwise { 
  0% { transform: rotate(0deg); }
  100% { transform: rotate(1turn); }
}

@keyframes counter-clockwise { 
  0% { transform: rotate(0deg); }
  100% { transform: rotate(-1turn); }
}

@-webkit-keyframes counter-clockwise { 
  0% { transform: rotate(0deg); }
  100% { transform: rotate(-1turn); }
}

@keyframes blinker { 
  50% { opacity: 0; }
}

@keyframes slideUpInSlideDownOut { 
  0% { transform: translateY(200px); visibility: visible; }
  10% { transform: translateY(0px); }
  90% { transform: translateY(0px); }
  100% { transform: translateY(200px); visibility: hidden; }
}

@keyframes slideDownInSlideUpOut { 
  0% { transform: translateY(-200px); }
  10% { transform: translateY(0px); visibility: visible; }
  80% { transform: translateY(0px); visibility: visible; }
  90% { transform: translateY(0px); visibility: hidden; }
  100% { transform: translateY(-200px); }
}

@keyframes fadeInFadeOut { 
  0% { opacity: 0; }
  10% { opacity: 1; }
  90% { opacity: 1; }
  100% { opacity: 0; }
}

@keyframes fadeInDown { 
  0% { opacity: 0; transform: translateY(0px); }
  100% { opacity: 1; transform: translateY(15px); }
}

@keyframes fadeIn { 
  0% { opacity: 0; }
  100% { opacity: 1; }
}

