.summary tr.account-headliners { font-size: 9.333px; color: rgb(102, 102, 102); }

.summary .account-headliners .one, .summary .account-headliners .two, .summary .account-headliners .three { padding-top: 16px; }

.summary .account-headliners .one, .summary .account-type { padding-bottom: 8px; text-align: left; width: 50%; padding-left: 16px; vertical-align: middle; }

.summary .account-headliners .two, .summary .account-headliners .three, .summary .balance-amount { padding-bottom: 8px; text-align: right; width: 25%; padding-right: 16px; vertical-align: middle; }

.summary .account-type, .summary .balance-amount { font-size: 13.333px; }

.summary tr.footer-row .footer-cell { padding: 4px; }

.secondary-account.headline { font-size: 21.333px; text-align: left; padding-top: 9px; padding-bottom: 9px; padding-left: 17px; margin-bottom: 10px; color: rgb(255, 255, 255); font-weight: bold; background-color: rgb(22, 43, 117); }

.year-to-date-period { font-weight: normal; font-size: 13.333px; }

