.year-to-date-summary.summary-table-wrapper .summary-table { width: 100%; }

.year-to-date-summary.summary-table-wrapper .year-to-date-summary.account-summary { height: 180px; width: 718px; }

.year-to-date-summary.summary-table-wrapper .year-to-date-summary.account-summary td { text-align: right; width: 22%; }

.year-to-date-summary.summary-table-wrapper .year-to-date-summary.account-summary td:first-child { text-align: left; width: 33%; }

.year-to-date-summary.summary-table-wrapper .year-to-date-summary.account-summary .as-headliner:nth-child(3) { padding-right: 0px; }

.summary-table-wrapper .summary-table { position: absolute; width: 353px; }

.summary-table-wrapper .fee-summary-table { position: absolute; left: 355px; width: 353px; }

.fee-summary-disclaimer { font-size: 11px; margin-bottom: 16px; }

table.account-summary { border: 1px solid rgb(102, 102, 102); width: 100%; padding: 0px; border-collapse: separate; border-spacing: 0px; height: 240px; }

table.account-summary-without-fee-summary { height: 170px !important; }

table.account-summary.fee-table { margin-left: 10px; }

.account-summary .title-row { height: 40px; margin-left: 16px; }

.account-summary .header-row { height: 18px; }

.account-summary .footer-row { height: auto; }

.account-summary .summary-title { height: 40px; font-size: 17.5px; font-weight: bold; color: rgb(22, 43, 117); text-align: left; vertical-align: middle; padding-left: 16px; }

.account-summary .as-headliner { height: 18px; font-size: 10px; color: rgb(102, 102, 102); text-align: right; vertical-align: middle; border-top: 1px solid rgb(102, 102, 102); padding-right: 16px; font-weight: normal; }

table.account-summary tr:nth-child(2n+2):not(:last-child) { height: 20px; background-color: rgb(255, 255, 255); border-right: 1px solid rgb(102, 102, 102); }

table.account-summary tr:nth-child(2n+3):not(:last-child) { height: 20px; background-color: rgb(204, 204, 204); border-right: 1px solid rgb(102, 102, 102); }

table.account-summary tr td { height: 20px; font-size: 13.333px; vertical-align: middle; }

table.account-summary td:nth-child(2n+1) { text-align: left; width: 70%; padding-left: 16px; }

table.account-summary td:nth-child(2n+2) { text-align: right; width: 30%; padding-right: 16px; }

.account-summary .footer-cell { height: auto; font-size: 10px; vertical-align: bottom; padding-bottom: 5px; padding-left: 16px; }

table.account-details { width: 100%; margin-bottom: 25px; }

.account-details .header-area { font-size: 21.333px; font-weight: bold; color: rgb(22, 43, 117); padding: 0px 0px 5.333px; text-align: left; }

.account-details .headline { font-size: 18.666px; padding: 10.666px 0px; text-align: left; }

.account-details tr.ad-headliner td { font-size: 9.333px; color: rgb(102, 102, 102); }

.account-details tr.ad-data td { font-size: 13.333px; padding: 3px 0px; }

.account-details .ad-headliner .ad-type.one, .account-details .ad-data .ad-info.date { text-align: left; width: 15%; }

.account-details .ad-headliner .ad-type.two, .account-details .ad-data .ad-info.descrip { width: 55%; }

.account-details .ad-headliner .ad-type.three, .account-details .ad-data .ad-info.trans { width: 15%; }

.account-details .ad-headliner .ad-type.four, .account-details .ad-data .ad-info.amount { text-align: right; width: 15%; }

table.legal-copy { text-align: left; width: 100%; }

h1.lc-header { font-size: 18.666px; padding: 0px; }

p, .lc-content { font-size: 10.666px; }

.questions { font-weight: bold; color: rgb(22, 43, 117); font-size: 13.333px; text-align: left; width: 70%; margin-top: 30px; margin-bottom: 30px; }

.print-date { font-size: 12px; text-align: right; margin-top: 30px; margin-bottom: 30px; }

.date { font-size: 10.666px; text-align: right; width: 20%; }

.page-number { font-size: 10.666px; padding-left: 50px; text-align: right; width: 10%; }

.table-footer { width: 100%; }
