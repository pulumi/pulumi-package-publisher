eader-row">                                <th class="as-headliner"></th>                                <th class="as-headliner">MAIN ACCOUNT FEES</th>                                <th class="as-headliner">SUBACCOUNT FEES*</th>                                <th class="as-headliner">TOTAL</th>                            </tr>                                    <tr>                                        <td>Monthly Fees</td>                                        <td>-$7.38</td>                                        <td>$0.00</td>                                        <td>-$7.38</td>                                    </tr>                                <tr>                                    <td>Currency Conversion Fees</td>                                    <td>$0.00</td>                                    <td>$0.00</td>                                    <td>$0.00</td>                                </tr>                                                            <tr>                                    <td>Card Replacement Fees</td>                                    <td>$0.00</td>                                    <td>$0.00</td>                                    <td>$0.00</td>                                </tr>                                <tr>                                    <td>Cash Withdrawal Fees</td>                                    <td>$0.00</td>                                    <td>$0.00</td>                                    <td>$0.00</td>                                </tr>                                <tr>                                    <td>Money Transfer Fees</td>                                    <td>$0.00</td>                                    <td>$0.00</td>                                    <td>$0.00</td>                                </tr>                            <tr>                                <td>ATM Fees</td>                                <td>-$21.00</td>                                <td>$0.00</td>     